import random
digits = '0123456789'
lowercase_letters = 'abcdefghijklmnopqrstuvwxyz'
uppercase_letters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'
punctuation = '!#$%&*+-=?@^_.'
chars = ''

kol_psw = int(input('Сколько паролей вы хотите создать? (введите число) - '))
len_psw = int(input('Какая нужна длина пароля? (введите одно число) - '))
psw_digits = input('Включать цифры (0123456789) в пароль? (введите "д" - если ДА, "н" - если НЕТ) - ')
if psw_digits == 'д':
    chars += digits
psw_upper = input('Включать прописные буквы (ABCDEFGHIJKLMNOPQRSTUVWXYZ) в пароль? (введите "д" - если ДА, "н" - если НЕТ) - ')
if psw_upper == 'д':
    chars += uppercase_letters
psw_lower = input('Включать строчные буквы (abcdefghijklmnopqrstuvwxyz) в пароль? (введите "д" - если ДА, "н" - если НЕТ) - ')
if psw_digits == 'д':
    chars += lowercase_letters
psw_sym = input('Включать символы (!#$%&*+-=?@^_?) в пароль? (введите "д" - если ДА, "н" - если НЕТ) - ')
if psw_sym == 'д':
    chars += punctuation
psw_except = input('Исключить неоднозначные символы (il1Lo0O) из пароля? (введите "д" - если ДА, "н" - если НЕТ) - ')
if psw_except == 'д':
    s =set('il1Lo0O')
    chars = "".join([c for c in chars if c not in s])

#
def generate_password(length, chars):
    password = ''
    for i in range(length):
        password += random.choice(chars)
    return password
# 
for i in range(kol_psw):
    print(generate_password(len_psw, chars))
